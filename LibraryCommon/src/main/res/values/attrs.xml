<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="Rotate3dAnimation">
        <attr name="rollType" format="enum">
            <enum name="x" value="0"/>
            <enum name="y" value="1"/>
            <enum name="z" value="2"/>
        </attr>
        <attr name="fromDeg" format="float" />
        <attr name="toDeg" format="float" />
        <attr name="pivotX" format="fraction"/>
        <attr name="pivotY" format="fraction" />
    </declare-styleable>

    <declare-styleable name="OvalImageView">
        <attr name="roundingRadius" format="integer" />
        <attr name="roundingType">
            <flag name="all" value="0" />
            <flag name="top" value="1" />
            <flag name="bottom" value="2" />
            <flag name="left" value="3" />
            <flag name="right" value="4" />
        </attr>

    </declare-styleable>
</resources>